{
  "name": "Where's the coin?",
  "version": "1.0.0",
  "content_scripts":[
    {
      "matches": ["*://*/*"],
      "js": ["script.js"]
    }
  ],
  "description": "Find the coin on the webpage!",
  "manifest_version": 3,
  "author": "Sam and Erika",
  "web_accessible_resources": [{
    "matches": ["<all_urls>"],
    "resources": ["coin.gif"]
  }],
  "action":{
      "default_popup": "index.html",
      "default_title": "Where's the coin?"
  }

}